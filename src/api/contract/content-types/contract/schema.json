{
  "kind": "collectionType",
  "collectionName": "contracts",
  "info": {
    "singularName": "contract",
    "pluralName": "contracts",
    "displayName": "Contract",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "NameVersion": {
      "type": "string",
      "regex": "^[A-Za-z]+@[0-9]+\\.[0-9]+\\.[0-9]+(?:-[A-Za-z0-9]+)?$",
      "required": true,
      "unique": true,
      "default": "<ContractName>@1.0.0"
    },
    "ABI": {
      "type": "json"
    },
    "ByteCode": {
      "type": "json"
    },
    "ReleaseNotes": {
      "type": "text"
    },
    "GitLink": {
      "type": "string"
    }
  }
}
